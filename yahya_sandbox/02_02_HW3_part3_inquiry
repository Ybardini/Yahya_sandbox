      "outputs": [],
      "source": [
        "##Testing for block_size > len(seq)\n",
        "#GC_blocks('AAAACCCC', 1000)\n",
        "##Testing for block_size < len(seq)\n",
        "#GC_blocks('ATGACTACGT', 4)"
      ]
